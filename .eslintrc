{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
        "import",
        "jsx-a11y",
        "react"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
}